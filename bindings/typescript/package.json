{
  "name": "@flood-protocol/contracts",
  "version": "0.3.0",
  "description": "Typescript bindings to Flood Protocol contracts",
  "main": "lib/index.ts",
  "scripts": {
    "generate": "rm -rf typescript && npx typechain --target ethers-v5 --out-dir lib \"../../out/**/!(*(*.t|*.s|*Fixture?(s)|Test|Vm|Script).sol)/*.json\"",
    "prepare": "patch-package"
  },
  "module": "lib/index.ts",
  "types": "lib/index.ts",
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fulmin-labs/flood-contracts.git"
  },
  "keywords": [
    "ethereum",
    "flood",
    "dex",
    "trading"
  ],
  "author": "francesco@fulminlabs.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fulmin-labs/flood-contracts/issues"
  },
  "homepage": "https://github.com/fulmin-labs/flood-contracts#readme",
  "devDependencies": {
    "@typechain/ethers-v5": "^10.1.0",
    "patch-package": "^6.4.7",
    "typechain": "^8.1.0"
  }
}
